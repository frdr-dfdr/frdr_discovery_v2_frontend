<footer role="“contentinfo”">
  <div id="geo-footer-container">
    <div id="geo-footer">
        <ul>
            <li>
              <div id="geo-footer-img" class="span2">
                <%= link_to t('geoblacklight.home.portage') do %>
                  <%= image_tag "portage_logo.png", alt: "Portage", height: 45 %>
                <% end %>
              </div>
            </li>
            <li>
              <div id="geo-footer-img" class="span2">
                <%= link_to 'https://www.ubc.ca/' do %>
                  <%= image_tag "ubc_logo.png", alt: "University Of British Columbia", height: 60 %>
                <% end %>
              </div>
            </li>
            <li>
              <div id="geo-footer-img" class="span2">
                <%= link_to t('geoblacklight.home.canarie') do %>
                  <%= image_tag "canarie.png", alt: "CANARIE", height: 60 %>
                <% end %>
              </div>
            </li>
            <li>
              <div id="geo-footer-img" class="span2">
                <%= link_to t('geoblacklight.home.ndrio') do %>
                  <%= image_tag "ndrio_logo.svg", alt: "Portage", height: 75 %>
                <% end %>
              </div>
            </li>
            <li>
              <div id="geo-footer-img" class="span2">
                <%= link_to t('geoblacklight.home.cc') do %>
                  <%= image_tag "logo_cc_enfr.png", alt: "Compute Canada", height: 75 %>
                <% end %>
              </div>
            </li>
        </ul>
        <div class="container">
            <div class="col-md-12" style="text-align:left;">
                <footer id="footer-text">
                    <br>
                    © 2017 - 2021 <%= t('geoblacklight.home.ndrio_lib') %>
                    <br>
                    <%= t('geoblacklight.home.cc_ndrio_priv') %>
                    <br>
                    <ul>
                        <li><a href=<%= t('geoblacklight.home.priv_uri') %> target="_blank"><%= t('geoblacklight.home.priv') %></a></li>
                        <br>
                        <li><span id="esri"><sup><%= t('geoblacklight.home.map_attrib_num') %></sup><sub><%= t('geoblacklight.home.map_attrib') %></sub></span></li>
                     </ul>
                </footer>
            </div>
        </div>
    </div>
  </div>
</footer>
